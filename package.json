{
  "name": "allfilm_api",
  "version": "0.0.1",
  "main": "app.local.js",
  "repository": "https://github.com/RamenHoang/ALLFILM_API.git",
  "author": "RamenHoang, LieuLe, QuangLuu",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "nodemon --ignore tmp/ --inspect=0.0.0.0 --stack-size=2048 app.local.js",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:reset": "npx sequelize-cli db:migrate:undo:all",
    "db:seed": "npx sequelize-cli db:seed:all",
    "model:gen": "npx sequelize-cli model:generate",
    "migration:gen": "npx sequelize-cli migration:generate",
    "seed:gen": "npx sequelize-cli seed:generate",
    "lint": "eslint \"**/*.js\" --ignore-pattern node_modules/",
    "lint:fix": "npm run lint -- --fix"
  },
  "devDependencies": {
    "husky": "4.3.0",
    "nodemon": "1.19.4",
    "sequelize-cli": "5.5.1"
  },
  "dependencies": {
    "@hapi/joi": "16.1.8",
    "async": "3.2.0",
    "axios": "0.20.0",
    "bcrypt": "5.0.0",
    "body-parser": "1.19.0",
    "cheerio": "1.0.0-rc.3",
    "cls-hooked": "4.2.2",
    "commander": "4.1.1",
    "compression": "1.7.4",
    "cors": "2.8.5",
    "dotenv": "8.2.0",
    "dynamoose": "1.11.1",
    "es6-promise": "4.2.8",
    "eslint": "6.8.0",
    "eslint-config-airbnb-base": "14.2.0",
    "eslint-plugin-chai-friendly": "0.4.1",
    "eslint-plugin-import": "2.22.1",
    "express": "4.17.1",
    "express-oauth-server": "2.0.0",
    "jsonwebtoken": "8.5.1",
    "lint-staged": "10.2.4",
    "lodash": "4.17.20",
    "moment": "2.29.1",
    "moment-range": "4.0.2",
    "multer": "1.4.2",
    "mysql2": "2.2.5",
    "nodemailer": "6.5.0",
    "sequelize": "5.22.3",
    "uuid": "8.3.1",
    "winston": "3.3.3",
    "winston-daily-rotate-file": "4.5.0"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "git add"
    ]
  }
}
